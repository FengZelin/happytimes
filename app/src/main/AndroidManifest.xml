<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="soexample.umeng.com.happytimes">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoTitle_FullScreen">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleInstance"
            android:theme="@style/AppTheme.NoTitle_FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.loginandregister.RegisterActivity" />
        <activity android:name=".activity.PersonaldataActivity" />
        <activity android:name=".activity.CircleActivity" />
        <activity android:name=".activity.FootmarkActivity" />
        <activity android:name=".activity.PurseActivity" />
        <activity android:name=".activity.AddressActivity"></activity>
    </application>

</manifest>