<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/bg_wdf"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_168"
        android:background="@drawable/attrss"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="55dp"
            android:gravity="center">

            <TextView
                android:id="@+id/text_name"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="我是吃不胖的小姐姐"
                android:textSize="@dimen/dp_14" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="70dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:layout_marginLeft="@dimen/dp_24"
                android:src="@mipmap/grzl_wdf" />

            <LinearLayout
                android:id="@+id/text_Personaldata"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_24"
                android:layout_marginLeft="20dp"
                android:orientation="vertical">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="个人资料"
                    android:textColor="#666666"
                    android:textSize="@dimen/dp_14" />
            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:layout_marginLeft="@dimen/dp_68"
            android:layout_marginTop="@dimen/dp_3"
            android:layout_marginRight="24dp"
            android:background="#666666" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:layout_marginLeft="@dimen/dp_24"
                android:src="@mipmap/wdqz_wdf" />

            <LinearLayout
                android:id="@+id/text_circle"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_24"
                android:layout_marginLeft="20dp"
                android:orientation="vertical">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的圈子"
                    android:textColor="#666666"
                    android:textSize="@dimen/dp_14" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:layout_marginLeft="@dimen/dp_68"
            android:layout_marginTop="@dimen/dp_3"
            android:layout_marginRight="24dp"
            android:background="#666666" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:layout_marginLeft="@dimen/dp_24"
                android:src="@mipmap/zj_wdf" />

            <LinearLayout
                android:id="@+id/text_footmark"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_24"
                android:layout_marginLeft="20dp"
                android:orientation="vertical">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的足迹"
                    android:textColor="#666666"
                    android:textSize="@dimen/dp_14" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:layout_marginLeft="@dimen/dp_68"
            android:layout_marginTop="@dimen/dp_3"
            android:layout_marginRight="24dp"
            android:background="#666666" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:layout_marginLeft="@dimen/dp_24"
                android:src="@mipmap/wdqb_wdf" />

            <LinearLayout
                android:id="@+id/text_purse"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_24"
                android:layout_marginLeft="20dp"
                android:orientation="vertical">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的钱包"
                    android:textColor="#666666"
                    android:textSize="@dimen/dp_14" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:layout_marginLeft="@dimen/dp_68"
            android:layout_marginTop="@dimen/dp_3"
            android:layout_marginRight="24dp"
            android:background="#666666" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:layout_marginLeft="@dimen/dp_24"
                android:src="@mipmap/shdz_wdf" />

            <LinearLayout
                android:id="@+id/text_address"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_24"
                android:layout_marginLeft="20dp"
                android:orientation="vertical">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的收货地址"
                    android:textColor="#666666"
                    android:textSize="@dimen/dp_14" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:layout_marginLeft="@dimen/dp_68"
            android:layout_marginTop="@dimen/dp_3"
            android:layout_marginRight="24dp"
            android:background="#666666" />
    </LinearLayout>

    <ImageView
        android:id="@+id/image_head"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_130"
        android:src="@mipmap/touxiang_wdf" />
</RelativeLayout>